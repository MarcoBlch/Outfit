<!-- Help Center Page -->
<div class="min-h-screen theme-bg">
  <!-- Navigation -->
  <nav class="sticky top-0 z-50 theme-bg/80 backdrop-blur-sm border-b theme-border">
    <div class="mx-auto max-w-7xl px-4 sm:px-6 lg:px-8">
      <div class="flex h-16 items-center justify-between">
        <%= link_to root_path, class: "flex items-center space-x-2" do %>
          <%= image_tag 'logo_option_4.svg', class: 'w-8 h-8 rounded-lg', alt: 'OutfitMaker.ai' %>
          <span class="text-xl font-bold bg-gradient-primary bg-clip-text text-transparent">
            OutfitMaker.ai
          </span>
        <% end %>
        <div class="flex items-center space-x-4">
          <button type="button"
                  class="p-2 theme-text-secondary hover:theme-text transition-colors"
                  data-controller="theme-toggle"
                  data-action="click->theme-toggle#toggle"
                  aria-label="Toggle theme">
            <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke-width="1.5" stroke="currentColor" class="w-5 h-5">
              <path stroke-linecap="round" stroke-linejoin="round" d="M12 3v2.25m6.364.386-1.591 1.591M21 12h-2.25m-.386 6.364-1.591-1.591M12 18.75V21m-4.773-4.227-1.591 1.591M5.25 12H3m4.227-4.773L5.636 5.636M15.75 12a3.75 3.75 0 1 1-7.5 0 3.75 3.75 0 0 1 7.5 0Z" />
            </svg>
          </button>
          <%= link_to "Back to Home", root_path, class: "theme-text-secondary hover:theme-text font-medium transition" %>
        </div>
      </div>
    </div>
  </nav>

  <!-- Hero Section -->
  <div class="bg-gradient-to-br from-purple-500/10 via-pink-500/10 to-indigo-500/10 border-b theme-border">
    <div class="mx-auto max-w-7xl px-4 sm:px-6 lg:px-8 py-16 text-center">
      <h1 class="text-4xl sm:text-5xl font-bold theme-text mb-4 tracking-tight">
        How can we help you?
      </h1>
      <p class="text-xl theme-text-secondary max-w-2xl mx-auto mb-8">
        Search our help center or browse categories below
      </p>

      <!-- Search Bar -->
      <div class="max-w-2xl mx-auto">
        <div class="relative">
          <input type="text"
                 placeholder="Search for help articles..."
                 class="w-full px-6 py-4 pl-14 rounded-xl theme-card theme-border border theme-text placeholder:theme-text-secondary focus:ring-2 focus:ring-purple-500 focus:border-transparent transition text-lg shadow-lg">
          <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke-width="1.5" stroke="currentColor" class="absolute left-5 top-1/2 -translate-y-1/2 w-6 h-6 theme-text-secondary">
            <path stroke-linecap="round" stroke-linejoin="round" d="m21 21-5.197-5.197m0 0A7.5 7.5 0 1 0 5.196 5.196a7.5 7.5 0 0 0 10.607 10.607Z" />
          </svg>
        </div>
      </div>
    </div>
  </div>

  <!-- Content -->
  <div class="mx-auto max-w-7xl px-4 sm:px-6 lg:px-8 py-16">
    <!-- Quick Links -->
    <div class="mb-16">
      <h2 class="text-2xl font-bold theme-text mb-6 text-center">Popular Topics</h2>
      <div class="flex flex-wrap justify-center gap-3">
        <%= link_to faq_path, class: "px-4 py-2 theme-card border theme-border rounded-full hover:bg-purple-500 hover:text-white transition" do %>
          Getting Started
        <% end %>
        <%= link_to faq_path, class: "px-4 py-2 theme-card border theme-border rounded-full hover:bg-purple-500 hover:text-white transition" do %>
          AI Outfit Suggestions
        <% end %>
        <%= link_to faq_path, class: "px-4 py-2 theme-card border theme-border rounded-full hover:bg-purple-500 hover:text-white transition" do %>
          Premium Features
        <% end %>
        <%= link_to faq_path, class: "px-4 py-2 theme-card border theme-border rounded-full hover:bg-purple-500 hover:text-white transition" do %>
          Billing & Subscriptions
        <% end %>
      </div>
    </div>

    <!-- Help Categories -->
    <div class="grid grid-cols-1 md:grid-cols-2 lg:grid-cols-3 gap-6 mb-16">
      <!-- Category 1: Getting Started -->
      <div class="theme-card rounded-xl p-6 border theme-border hover:border-purple-500 transition group">
        <div class="w-12 h-12 rounded-lg bg-gradient-to-br from-purple-500 to-pink-500 flex items-center justify-center mb-4 group-hover:scale-110 transition">
          <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke-width="1.5" stroke="currentColor" class="w-6 h-6 text-white">
            <path stroke-linecap="round" stroke-linejoin="round" d="M15.59 14.37a6 6 0 0 1-5.84 7.38v-4.8m5.84-2.58a14.98 14.98 0 0 0 6.16-12.12A14.98 14.98 0 0 0 9.631 8.41m5.96 5.96a14.926 14.926 0 0 1-5.841 2.58m-.119-8.54a6 6 0 0 0-7.381 5.84h4.8m2.581-5.84a14.927 14.927 0 0 0-2.58 5.84m2.699 2.7c-.103.021-.207.041-.311.06a15.09 15.09 0 0 1-2.448-2.448 14.9 14.9 0 0 1 .06-.312m-2.24 2.39a4.493 4.493 0 0 0-1.757 4.306 4.493 4.493 0 0 0 4.306-1.758M16.5 9a1.5 1.5 0 1 1-3 0 1.5 1.5 0 0 1 3 0Z" />
          </svg>
        </div>
        <h3 class="text-xl font-semibold theme-text mb-2">Getting Started</h3>
        <p class="theme-text-secondary mb-4 text-sm">
          Learn the basics of setting up your account and adding your first items
        </p>
        <ul class="space-y-2">
          <li>
            <%= link_to faq_path, class: "text-sm theme-text-secondary hover:text-purple-600 dark:hover:text-purple-400 flex items-center" do %>
              <svg class="w-4 h-4 mr-2 flex-shrink-0" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 5l7 7-7 7" />
              </svg>
              Creating your account
            <% end %>
          </li>
          <li>
            <%= link_to faq_path, class: "text-sm theme-text-secondary hover:text-purple-600 dark:hover:text-purple-400 flex items-center" do %>
              <svg class="w-4 h-4 mr-2 flex-shrink-0" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 5l7 7-7 7" />
              </svg>
              Adding wardrobe items
            <% end %>
          </li>
          <li>
            <%= link_to faq_path, class: "text-sm theme-text-secondary hover:text-purple-600 dark:hover:text-purple-400 flex items-center" do %>
              <svg class="w-4 h-4 mr-2 flex-shrink-0" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 5l7 7-7 7" />
              </svg>
              Taking great photos
            <% end %>
          </li>
        </ul>
      </div>

      <!-- Category 2: Wardrobe Management -->
      <div class="theme-card rounded-xl p-6 border theme-border hover:border-purple-500 transition group">
        <div class="w-12 h-12 rounded-lg bg-gradient-to-br from-pink-500 to-rose-500 flex items-center justify-center mb-4 group-hover:scale-110 transition">
          <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke-width="1.5" stroke="currentColor" class="w-6 h-6 text-white">
            <path stroke-linecap="round" stroke-linejoin="round" d="M20.25 7.5l-.625 10.632a2.25 2.25 0 0 1-2.247 2.118H6.622a2.25 2.25 0 0 1-2.247-2.118L3.75 7.5M10 11.25h4M3.375 7.5h17.25c.621 0 1.125-.504 1.125-1.125v-1.5c0-.621-.504-1.125-1.125-1.125H3.375c-.621 0-1.125.504-1.125 1.125v1.5c0 .621.504 1.125 1.125 1.125Z" />
          </svg>
        </div>
        <h3 class="text-xl font-semibold theme-text mb-2">Wardrobe Management</h3>
        <p class="theme-text-secondary mb-4 text-sm">
          Organize, edit, and manage your clothing collection
        </p>
        <ul class="space-y-2">
          <li>
            <%= link_to faq_path, class: "text-sm theme-text-secondary hover:text-purple-600 dark:hover:text-purple-400 flex items-center" do %>
              <svg class="w-4 h-4 mr-2 flex-shrink-0" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 5l7 7-7 7" />
              </svg>
              Organizing with tags
            <% end %>
          </li>
          <li>
            <%= link_to faq_path, class: "text-sm theme-text-secondary hover:text-purple-600 dark:hover:text-purple-400 flex items-center" do %>
              <svg class="w-4 h-4 mr-2 flex-shrink-0" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 5l7 7-7 7" />
              </svg>
              Editing item details
            <% end %>
          </li>
          <li>
            <%= link_to faq_path, class: "text-sm theme-text-secondary hover:text-purple-600 dark:hover:text-purple-400 flex items-center" do %>
              <svg class="w-4 h-4 mr-2 flex-shrink-0" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 5l7 7-7 7" />
              </svg>
              Deleting items
            <% end %>
          </li>
        </ul>
      </div>

      <!-- Category 3: Outfits & AI -->
      <div class="theme-card rounded-xl p-6 border theme-border hover:border-purple-500 transition group">
        <div class="w-12 h-12 rounded-lg bg-gradient-to-br from-indigo-500 to-purple-500 flex items-center justify-center mb-4 group-hover:scale-110 transition">
          <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke-width="1.5" stroke="currentColor" class="w-6 h-6 text-white">
            <path stroke-linecap="round" stroke-linejoin="round" d="M9.813 15.904 9 18.75l-.813-2.846a4.5 4.5 0 0 0-3.09-3.09L2.25 12l2.846-.813a4.5 4.5 0 0 0 3.09-3.09L9 5.25l.813 2.846a4.5 4.5 0 0 0 3.09 3.09L15.75 12l-2.846.813a4.5 4.5 0 0 0-3.09 3.09ZM18.259 8.715 18 9.75l-.259-1.035a3.375 3.375 0 0 0-2.455-2.456L14.25 6l1.036-.259a3.375 3.375 0 0 0 2.455-2.456L18 2.25l.259 1.035a3.375 3.375 0 0 0 2.456 2.456L21.75 6l-1.035.259a3.375 3.375 0 0 0-2.456 2.456Z" />
          </svg>
        </div>
        <h3 class="text-xl font-semibold theme-text mb-2">Outfits & AI</h3>
        <p class="theme-text-secondary mb-4 text-sm">
          Get the most out of AI-powered outfit suggestions
        </p>
        <ul class="space-y-2">
          <li>
            <%= link_to faq_path, class: "text-sm theme-text-secondary hover:text-purple-600 dark:hover:text-purple-400 flex items-center" do %>
              <svg class="w-4 h-4 mr-2 flex-shrink-0" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 5l7 7-7 7" />
              </svg>
              How AI suggestions work
            <% end %>
          </li>
          <li>
            <%= link_to faq_path, class: "text-sm theme-text-secondary hover:text-purple-600 dark:hover:text-purple-400 flex items-center" do %>
              <svg class="w-4 h-4 mr-2 flex-shrink-0" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 5l7 7-7 7" />
              </svg>
              Creating manual outfits
            <% end %>
          </li>
          <li>
            <%= link_to faq_path, class: "text-sm theme-text-secondary hover:text-purple-600 dark:hover:text-purple-400 flex items-center" do %>
              <svg class="w-4 h-4 mr-2 flex-shrink-0" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 5l7 7-7 7" />
              </svg>
              Customizing preferences
            <% end %>
          </li>
        </ul>
      </div>

      <!-- Category 4: Account Settings -->
      <div class="theme-card rounded-xl p-6 border theme-border hover:border-purple-500 transition group">
        <div class="w-12 h-12 rounded-lg bg-gradient-to-br from-cyan-500 to-blue-500 flex items-center justify-center mb-4 group-hover:scale-110 transition">
          <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke-width="1.5" stroke="currentColor" class="w-6 h-6 text-white">
            <path stroke-linecap="round" stroke-linejoin="round" d="M17.982 18.725A7.488 7.488 0 0 0 12 15.75a7.488 7.488 0 0 0-5.982 2.975m11.963 0a9 9 0 1 0-11.963 0m11.963 0A8.966 8.966 0 0 1 12 21a8.966 8.966 0 0 1-5.982-2.275M15 9.75a3 3 0 1 1-6 0 3 3 0 0 1 6 0Z" />
          </svg>
        </div>
        <h3 class="text-xl font-semibold theme-text mb-2">Account Settings</h3>
        <p class="theme-text-secondary mb-4 text-sm">
          Manage your profile, preferences, and security
        </p>
        <ul class="space-y-2">
          <li>
            <%= link_to faq_path, class: "text-sm theme-text-secondary hover:text-purple-600 dark:hover:text-purple-400 flex items-center" do %>
              <svg class="w-4 h-4 mr-2 flex-shrink-0" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 5l7 7-7 7" />
              </svg>
              Updating profile
            <% end %>
          </li>
          <li>
            <%= link_to faq_path, class: "text-sm theme-text-secondary hover:text-purple-600 dark:hover:text-purple-400 flex items-center" do %>
              <svg class="w-4 h-4 mr-2 flex-shrink-0" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 5l7 7-7 7" />
              </svg>
              Changing password
            <% end %>
          </li>
          <li>
            <%= link_to faq_path, class: "text-sm theme-text-secondary hover:text-purple-600 dark:hover:text-purple-400 flex items-center" do %>
              <svg class="w-4 h-4 mr-2 flex-shrink-0" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 5l7 7-7 7" />
              </svg>
              Privacy settings
            <% end %>
          </li>
        </ul>
      </div>

      <!-- Category 5: Billing & Subscriptions -->
      <div class="theme-card rounded-xl p-6 border theme-border hover:border-purple-500 transition group">
        <div class="w-12 h-12 rounded-lg bg-gradient-to-br from-green-500 to-emerald-500 flex items-center justify-center mb-4 group-hover:scale-110 transition">
          <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke-width="1.5" stroke="currentColor" class="w-6 h-6 text-white">
            <path stroke-linecap="round" stroke-linejoin="round" d="M2.25 8.25h19.5M2.25 9h19.5m-16.5 5.25h6m-6 2.25h3m-3.75 3h15a2.25 2.25 0 0 0 2.25-2.25V6.75A2.25 2.25 0 0 0 19.5 4.5h-15a2.25 2.25 0 0 0-2.25 2.25v10.5A2.25 2.25 0 0 0 4.5 19.5Z" />
          </svg>
        </div>
        <h3 class="text-xl font-semibold theme-text mb-2">Billing & Subscriptions</h3>
        <p class="theme-text-secondary mb-4 text-sm">
          Questions about pricing, payments, and subscriptions
        </p>
        <ul class="space-y-2">
          <li>
            <%= link_to faq_path, class: "text-sm theme-text-secondary hover:text-purple-600 dark:hover:text-purple-400 flex items-center" do %>
              <svg class="w-4 h-4 mr-2 flex-shrink-0" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 5l7 7-7 7" />
              </svg>
              Premium features
            <% end %>
          </li>
          <li>
            <%= link_to faq_path, class: "text-sm theme-text-secondary hover:text-purple-600 dark:hover:text-purple-400 flex items-center" do %>
              <svg class="w-4 h-4 mr-2 flex-shrink-0" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 5l7 7-7 7" />
              </svg>
              Canceling subscription
            <% end %>
          </li>
          <li>
            <%= link_to faq_path, class: "text-sm theme-text-secondary hover:text-purple-600 dark:hover:text-purple-400 flex items-center" do %>
              <svg class="w-4 h-4 mr-2 flex-shrink-0" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 5l7 7-7 7" />
              </svg>
              Payment methods
            <% end %>
          </li>
        </ul>
      </div>

      <!-- Category 6: Troubleshooting -->
      <div class="theme-card rounded-xl p-6 border theme-border hover:border-purple-500 transition group">
        <div class="w-12 h-12 rounded-lg bg-gradient-to-br from-orange-500 to-red-500 flex items-center justify-center mb-4 group-hover:scale-110 transition">
          <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke-width="1.5" stroke="currentColor" class="w-6 h-6 text-white">
            <path stroke-linecap="round" stroke-linejoin="round" d="M11.42 15.17 17.25 21A2.652 2.652 0 0 0 21 17.25l-5.877-5.877M11.42 15.17l2.496-3.03c.317-.384.74-.626 1.208-.766M11.42 15.17l-4.655 5.653a2.548 2.548 0 1 1-3.586-3.586l6.837-5.63m5.108-.233c.55-.164 1.163-.188 1.743-.14a4.5 4.5 0 0 0 4.486-6.336l-3.276 3.277a3.004 3.004 0 0 1-2.25-2.25l3.276-3.276a4.5 4.5 0 0 0-6.336 4.486c.091 1.076-.071 2.264-.904 2.95l-.102.085m-1.745 1.437L5.909 7.5H4.5L2.25 3.75l1.5-1.5L7.5 4.5v1.409l4.26 4.26m-1.745 1.437 1.745-1.437m6.615 8.206L15.75 15.75M4.867 19.125h.008v.008h-.008v-.008Z" />
          </svg>
        </div>
        <h3 class="text-xl font-semibold theme-text mb-2">Troubleshooting</h3>
        <p class="theme-text-secondary mb-4 text-sm">
          Solve common issues and technical problems
        </p>
        <ul class="space-y-2">
          <li>
            <%= link_to faq_path, class: "text-sm theme-text-secondary hover:text-purple-600 dark:hover:text-purple-400 flex items-center" do %>
              <svg class="w-4 h-4 mr-2 flex-shrink-0" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 5l7 7-7 7" />
              </svg>
              Upload issues
            <% end %>
          </li>
          <li>
            <%= link_to faq_path, class: "text-sm theme-text-secondary hover:text-purple-600 dark:hover:text-purple-400 flex items-center" do %>
              <svg class="w-4 h-4 mr-2 flex-shrink-0" fill="none" stroke="currentColor" viewBox="0 0 24 24">
